<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="textColor" format="reference|color"/>
    <attr name="leftMargin" format="dimension"/>

    <declare-styleable name="TopBar">
        <attr name="leftText" format="string"/>
        <attr name="rightText1" format="string"/>
        <attr name="rightText2" format="string"/>
        <attr name="textColor"/>
        <attr name="leftMargin"/>
        <attr name="rightMargin1" format="dimension"/>
        <attr name="rightMargin2" format="dimension"/>
        <attr name="leftSize" format="dimension"/>
        <attr name="rightSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SettingBar">
        <attr name="logo" format="reference"/>
        <attr name="text" format="string"/>
        <attr name="arrow" format="reference"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor"/>
        <attr name="imageSize" format="dimension"/>
        <attr name="marginLeft" format="dimension"/>
        <attr name="marginRight" format="dimension"/>
    </declare-styleable>

</resources>
